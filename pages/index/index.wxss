/**index.wxss**/
.topbar {
  width: 100%;
  height: 45px;
  background-color: white;
  display: flex;
  margin-left: 0rpx;
  margin-right: 0rpx !important;

overflow: hidden;
position: fixed;
top: 0;
left: 0;
z-index: 10;
}

.row {
  display: flex;
  justify-content: center;
  align-items: center;
}

.arrow-box {
  margin-left: 20rpx;

  height: 100%;
  /* background-color: #fe6969; */
  float: left;
}

.arrow-box image {
  width: 175rpx;
  height: 51rpx;
}


.input-box {
  width: 39%;
  height: 100%;
  /* background-color: #eee; */
  justify-content: center;
  align-items: center;
  float: left;
  margin-left: 3%;
}

.input-box input {
  font-size: 12px;
  color: #333;
  width: 93%;
  height: 30px;
  background-color: #f2f2f2;
  border-radius: 44rpx;
  padding-left: 7%;
}

.swip image {
  width: 100%;
}

.top-comend {
  width: 100%;
  height: 45px;
}

.tz {
  height: 110rpx;
  display: flex;
  margin-top: 120rpx;
  background-color: white;
  align-items: center;


}

.tz .header {
  display: flex;
  width: 50%;
}

.tz .head {
  display: flex;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: rgb(236, 32, 32);

}

.right {
  display: flex;
  width: 50%;
  justify-content: space-around;
}

.rightrow {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 20rpx;
}

.head image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;

}

.right image {
  width: 33rpx;
  height: 33rpx;
}

.header .info {
  display: flex;
  flex-direction: column;
  justify-content: center;

}

.info .name {
  font-size: 28rpx;
  font-weight: bold;
}

.info .add {
  font-size: 20rpx;
  color: #afafaf;
}

.right text {
  font-size: 22rpx;
  color: #afafaf;
}

.sales {
  margin-top: 35rpx;

  height: 550rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 16rpx 0px rgba(0, 0, 0, 0.18);
  border-radius: 30rpx;
}

.sales .title {
  width: 711rpx;
  height: 95rpx;
  background: rgba(239, 184, 78, 1);
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  display: flex;
  color: white;
  align-items: center;

}

.sales image {
  width: 50rpx;
  height: 50rpx;
  margin-left: 20rpx;
}

.xs {
  margin-left: 20rpx;
  width: 55rpx;
}

.sales .time {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  align-items: center;
}

.sx {
  margin-left: 20rpx;
  height: 50rpx;
  width: 1rpx;
  color: white;
}

.xs,
.num {
