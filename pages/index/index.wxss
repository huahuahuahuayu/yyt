  margin-left: 0rpx;
}

.pic {
  position: relative;

}

.click {
  width: 200rpx;
  height: 40rpx;
  background: rgba(0, 0, 0, 1);
  opacity: 0.49;
  border-radius: 20rpx;
  color: rgb(235, 233, 233);
  text-align: center;
  position: absolute;
  z-index: 1;
  right: 0rpx;
  top: 140rpx;
  font-size: 22rpx;
  line-height: 40rpx;
}

.sales .detail {
  display: flex;
  justify-content: space-between;
}

.left-detail {
  margin-left: 15rpx;
}

.right-detail {
